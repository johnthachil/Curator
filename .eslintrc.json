{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "experimentalDecorators":true,
      "jsx":true
    },
    "sourceType": "module"
  },
  "rules": {
    "react/jsx-filename-extension": 0
  },
  "plugins": ["react"],
  "extends": "airbnb"
}
